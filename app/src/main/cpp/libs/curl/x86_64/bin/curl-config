###########################################################################

prefix=/Users/rying/code/openssl-curl-android/curl/build/x86_64
exec_prefix=${prefix}
includedir=${prefix}/include
cppflag_curl_staticlib=-DCURL_STATICLIB

usage()
{
    cat <<EOF
Usage: curl-config [OPTION]

Available values for OPTION include:

  --built-shared says 'yes' if libcurl was built shared
  --ca        ca bundle install path
  --cc        compiler
  --cflags    pre-processor and compiler flags
  --checkfor [version] check for (lib)curl of the specified version
  --configure the arguments given to configure when building curl
  --features  newline separated list of enabled features
  --help      display this help and exit
  --libs      library linking information
  --prefix    curl install prefix
  --protocols newline separated list of enabled protocols
  --ssl-backends output the SSL backends libcurl was built to support
  --static-libs static libcurl library linking information
  --version   output version information
  --vernum    output the version information as a number (hexadecimal)
EOF

    exit $1
}

if test $# -eq 0; then
    usage 1
fi

while test $# -gt 0; do
    case "$1" in
    # this deals with options in the style
    # --option=value and extracts the value part
    # [not currently used]
    -*=*) value=`echo "$1" | sed 's/[-_a-zA-Z0-9]*=//'` ;;
    *) value= ;;
    esac

    case "$1" in
    --built-shared)
        echo no
        ;;

    --ca)
        echo 
        ;;

    --cc)
        echo "/Users/rying/Library/Android/sdk/ndk/21.1.6352462/toolchains/llvm/prebuilt/darwin-x86_64/bin/x86_64-linux-android21-clang"
        ;;

    --prefix)
        echo "$prefix"
        ;;

    --feature|--features)
        for feature in SSL AsynchDNS TLS-SRP HTTPS-proxy ""; do
            test -n "$feature" && echo "$feature"
        done
        ;;

    --protocols)
        for protocol in HTTP HTTPS; do
            echo "$protocol"
        done
        ;;

    --version)
        echo libcurl 7.70.0-DEV
        exit 0
        ;;

    --checkfor)
        checkfor=$2
        cmajor=`echo $checkfor | cut -d. -f1`
        cminor=`echo $checkfor | cut -d. -f2`
        # when extracting the patch part we strip off everything after a
        # dash as that's used for things like version 1.2.3-CVS
        cpatch=`echo $checkfor | cut -d. -f3 | cut -d- -f1`

        vmajor=`echo 7.70.0-DEV | cut -d. -f1`
        vminor=`echo 7.70.0-DEV | cut -d. -f2`
        # when extracting the patch part we strip off everything after a
        # dash as that's used for things like version 1.2.3-CVS
        vpatch=`echo 7.70.0-DEV | cut -d. -f3 | cut -d- -f1`

        if test "$vmajor" -gt "$cmajor"; then
            exit 0;
        fi
        if test "$vmajor" -eq "$cmajor"; then
            if test "$vminor" -gt "$cminor"; then
                exit 0
            fi
            if test "$vminor" -eq "$cminor"; then
                if test "$cpatch" -le "$vpatch"; then
                    exit 0
                fi
            fi
        fi

        echo "requested version $checkfor is newer than existing 7.70.0-DEV"
        exit 1
        ;;

    --vernum)
        echo 074600
        exit 0
        ;;

    --help)
        usage 0
        ;;

    --cflags)
        if test "X$cppflag_curl_staticlib" = "X-DCURL_STATICLIB"; then
          CPPFLAG_CURL_STATICLIB="-DCURL_STATICLIB "
        else
          CPPFLAG_CURL_STATICLIB=""
        fi
        if test "X${prefix}/include" = "X/usr/include"; then
          echo "$CPPFLAG_CURL_STATICLIB"
        else
          echo "${CPPFLAG_CURL_STATICLIB}-I${prefix}/include"
        fi
        ;;

    --libs)
        if test "X${exec_prefix}/lib" != "X/usr/lib" -a "X${exec_prefix}/lib" != "X/usr/lib64"; then
           CURLLIBDIR="-L${exec_prefix}/lib "
        else
           CURLLIBDIR=""
        fi
        if test "Xyes" = "Xyes"; then
          echo ${CURLLIBDIR}-lcurl -lssl -lcrypto -lssl -lcrypto
        else
          echo ${CURLLIBDIR}-lcurl
        fi
        ;;
    --ssl-backends)
        echo "OpenSSL"
        ;;

    --static-libs)
        if test "Xyes" != "Xno" ; then
          echo ${exec_prefix}/lib/libcurl.a -Wl,-s -Wl,-Bsymbolic -Wl,--gc-sections -L/Users/rying/code/openssl-curl-android/openssl/build/x86_64/lib -lssl -lcrypto -lssl -lcrypto
        else
          echo "curl was built with static libraries disabled" >&2
          exit 1
        fi
        ;;

    --configure)
        echo " '--host=x86_64-linux-android' '--target=x86_64-linux-android' '--prefix=/Users/rying/code/openssl-curl-android/curl/build/x86_64' '--with-ssl=/Users/rying/code/openssl-curl-android/curl/../openssl/build/x86_64' '--disable-shared' '--disable-verbose' '--disable-manual' '--disable-crypto-auth' '--disable-unix-sockets' '--disable-ares' '--disable-rtsp' '--disable-ipv6' '--disable-proxy' '--disable-versioned-symbols' '--enable-hidden-symbols' '--without-libidn' '--without-librtmp' '--without-zlib' '--disable-dict' '--disable-file' '--disable-ftp' '--disable-ftps' '--disable-gopher' '--disable-imap' '--disable-imaps' '--disable-pop3' '--disable-pop3s' '--disable-smb' '--disable-smbs' '--disable-smtp' '--disable-smtps' '--disable-telnet' '--disable-tftp' 'host_alias=x86_64-linux-android' 'target_alias=x86_64-linux-android' 'CC=/Users/rying/Library/Android/sdk/ndk/21.1.6352462/toolchains/llvm/prebuilt/darwin-x86_64/bin/x86_64-linux-android21-clang' 'CFLAGS=-Os -ffunction-sections -fdata-sections -fno-unwind-tables -fno-asynchronous-unwind-tables' 'LDFLAGS=-Wl,-s -Wl,-Bsymbolic -Wl,--gc-sections'"
        ;;

    *)
        echo "unknown option: $1"
        usage 1
        ;;
    esac
    shift
done

exit 0
